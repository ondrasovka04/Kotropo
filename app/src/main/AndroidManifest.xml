<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cz.ucenislovicek">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Učení slovíček"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UceniSlovicek"
        tools:replace="android:label">
        <activity
            android:name=".login.activity_listSkol"
            android:exported="false" />
        <activity
            android:name=".drawer_items.prehled.Prehled"
            android:exported="false" />
        <activity
            android:name=".drawer_items.test.Test"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.UceniSlovicek.NoActionBar" />
        <activity
            android:name=".login.LoginForm"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>